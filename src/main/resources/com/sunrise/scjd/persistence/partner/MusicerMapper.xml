	<select id="selectDrillDownload" parameterType="com.sunrise.base.dataEntity.IData" resultType="map">
		select down_cnt DOWNLOADCNT, 	<!-- 费用数 -->
		usr USERCNT,					<!-- 用户数 -->
		down_cnt * usr DOWNLOADAMOUNT	<!-- 下载量 -->
		from
		<if test="dateType=='day'">
			<choose>
				<when test="pageId==222">RPT_HZHB_RING_DOWN_MUSICAN_DM</when>
				<when test="pageId==223">RPT_HZHB_RINGBOX_DOWN_MUSIC_DM</when>
				<when test="pageId==224">RPT_HZHB_ZRING_DOWN_MUSICAN_DM</when>
				<when test="pageId==225">RPT_HZHB_SONG_DOWN_MUSICAN_DM</when>
				<when test="pageId==227">RPT_HZHB_SST_DOWN_MUSICAN_DM</when>
				<when test="pageId==228">RPT_HZHB_ZSBY_DOWN_MUSICAN_DM</when>
				<when test="pageId==229">RPT_HZHB_THSY_DOWN_MUSICAN_DM</when>
			</choose>
